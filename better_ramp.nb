Cell[BoxData[{
 RowBox[{
  RowBox[{"CHK", "=", 
   RowBox[{"{", "}"}]}], ";", 
  RowBox[{"curfr", "=", "1"}], ";", 
  RowBox[{"curt", "=", "0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Do", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"curexp", "=", 
     RowBox[{"EXPTIMES", "[", 
      RowBox[{"[", "curfr", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"avgbright", "=", 
     RowBox[{
      FractionBox["1", "curexp"], 
      RowBox[{"NIntegrate", "[", 
       RowBox[{
        RowBox[{"BrightnessWithTime", "[", 
         RowBox[{"t", ",", "min", ",", "max", ",", "rate"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "curt", ",", 
          RowBox[{"curt", "+", "curexp"}]}], "}"}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"prod", "=", 
     RowBox[{"iso", " ", "area", " ", "curexp", " ", "avgbright"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"AppendTo", "[", 
     RowBox[{"CHK", ",", "prod"}], "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"curt", "+=", 
     RowBox[{"(", 
      RowBox[{"curexp", "+", "pause"}], ")"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"curfr", "++"}], ";"}], ",", "\[IndentingNewLine]", "frames"}], 
  "]"}], "\[IndentingNewLine]", 
