Cell[BoxData[{
 RowBox[{
  RowBox[{"scale", "=", 
   RowBox[{"s", "/.", 
    RowBox[{"Solve", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"s", 
          RowBox[{"(", 
           RowBox[{"Total", "[", "EXPTIMES", "]"}], ")"}]}], "+", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"frames", "-", "1"}], ")"}], "pause"}]}], ")"}], 
       "\[Equal]", "elapsed"}], ",", "s"}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SCALEDEXPTIMES", "=", 
   RowBox[{
    RowBox[{"scale", "[", 
     RowBox[{"[", "1", "]"}], "]"}], "EXPTIMES"}]}], ";"}]}], "Input",
